{
  "name": "quark",
  "author": "Nishkal Kashyap <hello@nishkal.in>",
  "email": "hello@nishkal.in",
  "homepage": "https://quarkjs.io",
  "version": "0.4.0",
  "description": "Software sketchbook for rapid prototyping and development of your projects.",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "install": "npm install @quarkjs/api@latest",
    "land": "ts-node ./scripts/prepare-assets.ts && npm run pack && electron .",
    "ql": "npm run pack && electron .",
    "start": "ts-node ./scripts/prepare-assets.ts && npm run pack && electron . ./start.qrk",
    "qs": "npm run pack && electron . ./start.qrk",
    "test": "ts-node ./scripts/prepare-assets.ts && npm run pack && electron . ./test/__testing__fjdsbfkbsdibsdi__testing__testing.qrk",
    "qt": "electron . ./test/__testing__fjdsbfkbsdibsdi__testing__testing.qrk",
    "pack": "webpack --config webpack.config.js",
    "link-local": "npm link @quarkjs/api",
    "lic": "ts-node ./scripts/license.ts",
    "build-quick": "ts-node ./scripts/prepare-assets.ts && npm run pack && npm run lic && ts-node scripts/build.ts",
    "build-common": "npm install && npm run build-quick",
    "build-win": "npm run build-common && start .\\build",
    "build-linux": "ts-node ./scripts/build-linux.ts && cd ./../Quark-electron-linux && npm run build-common",
    "bw": "npm run build-win",
    "bl": "bash -c 'npm run build-linux'",
    "tar": "ts-node ./scripts/prepare-tar.ts && ts-node ./scripts/prepare-assets.ts",
    "release": "ts-node ./scripts/release.ts",
    "upload": "cross-env DELETE_FILES=no ts-node ./scripts/upload.ts",
    "notes": "ts-node ./scripts/create-release-notes.ts",
    "native": "electron-builder install-app-deps",
    "build-ci": "npm run build-quick",
    "test-ci": "ts-node ./scripts/test.ts",
    "upload-ci": "ts-node ./scripts/upload-ci.ts",
    "release-ci": "ts-node ./scripts/release-ci.ts",
    "release-ci-olddddddddddddd": "git checkout release && git merge master && git push && git checkout master",
    "release-master-all": "git checkout master-all && git merge master && git push && git checkout master"
  },
